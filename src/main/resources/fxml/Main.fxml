<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="616.0" prefWidth="881.0"
            xmlns="http://javafx.com/javafx/2.2" fx:controller="by.divin.weather.start.Main">
    <children>
        <TabPane layoutX="14.0" layoutY="14.0" prefHeight="588.0" prefWidth="853.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Common report">
                    <content>
                        <fx:include fx:id="commonReportTab" source="CommonReportTab.fxml"/>
                    </content>
                </Tab>
                <Tab text="Temperature tab">
                    <content>
                        <fx:include fx:id="temperatureChartReportTab" source="TemperatureChartReportTab.fxml"/>
                    </content>
                </Tab>
                <Tab text="Pressure tab">
                    <content>
                        <fx:include fx:id="pressureChartReportTab" source="PressureChartReportTab.fxml"/>
                    </content>
                </Tab>
                <Tab text="Year character">
                    <content>
                        <fx:include fx:id="yearCharacterReportTab" source="YearCharacterReportTab.fxml"/>
                    </content>
                </Tab>
                <Tab text="Conclusions">
                    <content>
                        <fx:include fx:id="conclusionReportTab" source="ConclusionReportTab.fxml"/>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
